[user]
	email = davydes@gmail.com
	name = davydes
[push]
	default = matching

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[core]
	excludesfile = ~/.gitignore
[http "https://git.flynn.molinos.ru"]
	sslCAInfo = ~/.flynn/ca-certs/default.pem
[credential "https://git.flynn.molinos.ru"]
	helper = ~/bin/flynn git-credentials
